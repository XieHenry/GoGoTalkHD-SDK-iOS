           $scope.callController.func._handlerUpdateNewpptPagingPage= function(){ //动态PPT是否能够翻页
                var data = {
                    action:"changeNewpptPagingPage" ,
            		newpptPagingPage:$rootScope.newpptPagingPage
                };
                ServiceNewPptAynamicPPT.postMessage(data);
            };           
		};

