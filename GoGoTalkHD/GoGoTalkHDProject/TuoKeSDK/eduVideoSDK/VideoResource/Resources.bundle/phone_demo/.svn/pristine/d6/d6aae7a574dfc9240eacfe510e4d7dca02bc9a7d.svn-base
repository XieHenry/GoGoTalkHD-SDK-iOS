                var data = {
                    action:"changePublishDynamicPptMediaPermission_video" ,
                    publishDynamicPptMediaPermission_video:$rootScope.publishDynamicPptMediaPermission_video
                };
                ServiceNewPptAynamicPPT.postMessage(data);
