                studentNameText:dataName
            }
            var signallingName = "dialShow" , assignId = "dialMesg",do_not_save=false ;
            $(document).trigger("sendDataToLiterallyEvent",[ null , testData , signallingName , assignId ,do_not_save]);
            $(".dial-teachComponent-turntable").css("transform",'rotate('+ i * 1000 +'deg)');
            var i=Math.floor(Math.random()*6+3);
            var dataName=$scope.nameArr
            var  signallingName = "dialTurn" , assignId = "dialTurn" ;
            $(document).trigger("sendDataToLiterallyEvent",[ null , testData , signallingName , assignId ]);
        }
        $scope.dialCloseFun = function(){
            $(".dial-teachComponent-bg").hide();
            $(".dial-studentComponent-bg").hide()
            var testData={
                dialClose:true,
            }, signallingName = "dialShow" , assignId = "dialMesg" ;
            $(document).trigger("deleteLiterallyDataEvent" , [null, testData , signallingName , assignId])
        }
        $scope.timerShowHandel = function(){
            if($rootScope.hasRole.roleChairman){
                $(".timer-teachTool-wrap").show();

                $(".teach-box-left").css("display","none");
                console.error($(".teach-box-left"))
            }if($rootScope.hasRole.roleStudent){
                $(".timer-teachTool-wrap").hide();
            }

        }
        $scope.turnplateShow = function(){

            if($rootScope.hasRole.roleChairman){
                $(".dial-teachComponent-bg").show();
                $(".teach-box-left").hide();

            }if($rootScope.hasRole.roleStudent){
                $(".dial-teachComponent-bg").hide();
            }


        }

        $scope.teacherToolChoose = function(){
            $(".teach-box-left").show();
        }

	});
});

