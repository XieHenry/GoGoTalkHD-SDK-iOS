        $scope.describingCircle=function(colorLength,colorArry) {
            var cv = document.getElementById("dial-teachComponent-turnContent");
            var ctx = cv.getContext("2d");
            var  clientWidth = document.documentElement.clientWidth;
            //根据设计图中的canvas画布的占比进行设置
            var canvasWidth = Math.floor(clientWidth*200/720);
            cv.setAttribute('width',canvasWidth+'px');
            cv.setAttribute('height',canvasWidth+'px');
            var startAngle = -360 / colorLength,
                step = 360 / colorLength;

            colorArry.forEach(function (value, index) {
                ctx.beginPath();
                ctx.fillStyle = value;
                ctx.moveTo(canvasWidth/2, canvasWidth/2);
                ctx.arc(canvasWidth/2, canvasWidth/2, canvasWidth/2, $scope.toRadian(startAngle), $scope.toRadian(startAngle += step));
                ctx.fill();
            });
        };
        $scope.describingCircleStudent=function(colorLength,colorArry) {
